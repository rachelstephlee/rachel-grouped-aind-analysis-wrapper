{
	"version": 1,
	"base_image": "codeocean/mambaforge3:24.5.0-0-python3.12.4-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY",
		"CODEOCEAN_API_TOKEN"
	],
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"pip": {
			"packages": [
				{
					"name": "aind-analysis-arch-result-access",
					"version": "0.7.1"
				},
				{
					"name": "awscli",
					"version": "1.38.3"
				},
				{
					"name": "boto3",
					"version": "1.37.3"
				},
				{
					"name": "git+https://github.com/AllenNeuralDynamics/aind-dynamic-foraging-basic-analysis.git@main#egg=aind-dynamic-foraging-basic-analysis"
				},
				{
					"name": "git+https://github.com/AllenNeuralDynamics/aind-dynamic-foraging-data-utils.git@e98970ad1c1fcb2e235fef53a0cbf7361a360236#egg=aind-dynamic-foraging-data-utils"
				},
				{
					"name": "git+https://github.com/AllenNeuralDynamics/analysis-pipeline-utils@8722a08fc651ee0d4773927d91a5378ec8449980#egg=analysis-pipeline-utils"
				},
				{
					"name": "ipykernel",
					"version": "6.30.0"
				},
				{
					"name": "ipympl",
					"version": "0.9.6"
				},
				{
					"name": "matplotlib",
					"version": "3.7.5"
				},
				{
					"name": "numpy",
					"version": "1.26.4"
				},
				{
					"name": "pandas",
					"version": "2.3.1"
				},
				{
					"name": "plotly",
					"version": "5.24.1"
				},
				{
					"name": "psutil",
					"version": "7.0.0"
				},
				{
					"name": "pydantic-settings",
					"version": "2.9.1"
				},
				{
					"name": "pypdf2",
					"version": "3.0.1"
				},
				{
					"name": "rupture",
					"version": "1.4.5"
				},
				{
					"name": "s3fs",
					"version": "2025.2.0"
				},
				{
					"name": "scikit-learn",
					"version": "1.7.1"
				},
				{
					"name": "scipy",
					"version": "1.16.0"
				},
				{
					"name": "seaborn",
					"version": "0.13.2"
				},
				{
					"name": "statsmodels",
					"version": "0.14.5"
				},
				{
					"name": "xarray",
					"version": "2025.7.1"
				},
				{
					"name": "zarr",
					"version": "2.18.6"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	},
	"env_variables": [
		{
			"name": "DOCDB_HOST",
			"value": "api.allenneuraldynamics.org"
		},
		{
			"name": "DOCDB_DATABASE",
			"value": "analysis"
		},
		{
			"name": "DOCDB_COLLECTION",
			"value": "dynamic-foraging-nm"
		},
		{
			"name": "CODEOCEAN_DOMAIN",
			"value": "codeocean.allenneuraldynamics.org"
		},
		{
			"name": "CODEOCEAN_EMAIL",
			"value": "rachel.lee@alleninstitute.org"
		},
		{
			"name": "ANALYSIS_BUCKET",
			"value": "aind-analysis-prod-o5171v/dynamic-foraging-nm"
		}
	]
}