{
	"version": 1,
	"base_image": "codeocean/mambaforge3:24.5.0-0-python3.12.4-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY",
		"CODEOCEAN_API_TOKEN"
	],
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"pip": {
			"packages": [
				{
					"name": "git+https://github.com/AllenNeuralDynamics/aind-analysis-results.git#egg=aind-analysis-results",
					"version": "ee8b95b18da98e96d2778b223cb05e41b9b65554"
				},
				{
					"name": "pydantic-settings",
					"version": "2.9.1"
				},
				{
					"name": "s3fs",
					"version": "0.4.2"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	},
	"env_variables": [
		{
			"name": "DOCDB_HOST",
			"value": "api.allenneuraldynamics.org"
		},
		{
			"name": "DOCDB_DATABASE",
			"value": "analysis"
		},
		{
			"name": "DOCDB_COLLECTION",
			"value": "architecture_example_v2"
		},
		{
			"name": "CODEOCEAN_DOMAIN",
			"value": "codeocean.allenneuraldynamics.org"
		},
		{
			"name": "CODEOCEAN_EMAIL",
			"value": "YOUR_EMAIL_HERE@alleninstitute.org"
		},
		{
			"name": "ANALYSIS_BUCKET",
			"value": "aind-scratch-data/analysis/architecture_example_v2"
		}
	]
}